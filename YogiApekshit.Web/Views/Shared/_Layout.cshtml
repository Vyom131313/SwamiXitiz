        function FormatDialog() {
            $('.awe-uidialog').removeClass("ui-dialog ui-widget ui-widget-content ui-corner-all ui-front ui-dialog-buttons ui-draggable")
                                //.addClass("modal splash")
                                .addClass("modal-dialog modal-content");

            // Title
            $('.ui-dialog-titlebar').removeClass("ui-dialog-titlebar ui-widget-header ui-corner-all ui-helper-clearfix ui-draggable-handle")
                                    .addClass("modal-header");
            $('.ui-dialog-title').removeClass("ui-dialog-title")
                                 .addClass("modal-title custom-font h3")
                                 .attr("style", "font-weight:bold");
            $('.ui-dialog-titlebar-close').remove();

            $('.awe-popup').removeClass("ui-dialog-content ui-widget-content")
                           .addClass("modal-body");

            // Form: Horizontal
            $('.awe-popup>form').addClass("form-horizontal");

            // Buttons
            $('.ui-dialog-buttonpane').removeClass("ui-dialog-buttonpane ui-widget-content ui-helper-clearfix")
                           .addClass("modal-footer");
            $('.ui-dialog-buttonset').removeClass("ui-dialog-buttonset");
            $('button').removeClass('ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only');
            $("button:contains('OK')").addClass('btn btn-greensea btn-ef btn-ef-3');
            $("button:contains('Cancel')").addClass('btn btn-lightred btn-ef btn-ef-3');

            // Datetime picker
            $('.awe-datepicker-field').addClass('input-icon right')
                                      .attr("style", "width:100%")
                                      .prepend("<i class='fa fa-calendar'></i>");
            $("input.hasDatepicker:text").removeClass('awe-display').addClass('form-control');
            $('button.awe-dpbtn').remove();

            $('select.awe-display').addClass('form-control');
        }
    </script>
    
